/*
//Question 6
#include <stdio.h>
#include <math.h>
#define pi 3.14

int main(void)
{
	char ich;
	double length,result;
    printf("s or S for volume of spehere, c or C for volume of cube\n");
	printf("Enter calculation type: ");
	scanf("%c",&ich);
	printf("Enter length: ");
	scanf("%lf",&length);


	switch (ich)
	{
	    case 's':case 'S':
		  
			  result=(4*pi*pow(length,3))/3;
			  printf("Volume of the %lf radius sphere is %lf\n",length,result);
		  break;
		  

		  case 'c':case 'C':

		      result=pow(length,3);
			  printf("Volume of the %lf length cube is %lf\n",length,result);

		  break;
		  
		  default:
		printf("Wrong type,\Try again.. \n");
	}
	return 0;
}
