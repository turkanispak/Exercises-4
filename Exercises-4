	if (kwh<300)
	{
		cost=9*kwh;
		printf("Total cost is %d cents\n",cost);
	}
	else if (300<kwh && kwh<600)
	{
		xkwh=kwh-300;
		xcost=(xkwh*8) + (9*300);
		printf("Total cost is %d cents\n",xcost);
	}
	else if (600<kwh && kwh<1000)
	{
		xkwh=kwh-300;
		xcost=(xkwh*6) + (9*300);
