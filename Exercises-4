	scanf("%c",&grocery);
	printf("Enter how many: ");
	scanf("%d",&many);

	switch (grocery)
	{
	    case 'a':case 'A':
		  
			  cost=10*many;
			  printf("%d many apples cost %d cents\n",many,cost);
		  break;
		  

		  case 'b':case 'B':
		  
			  cost=12*many;
			  printf("%d many bananas cost %d cents\n",many,cost);
		  break;
		  

		  case 'c':case 'C':
		  
			  cost=7*many;
			  printf("%d many carrots cost %d cents\n",many,cost);
		  break;
		  
		  default:
		printf("Wrong type,\n enter a for apples, b for bananas and c for carrots) \n");
